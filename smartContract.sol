pragma solidity ^0.4.4;

contract TxRecord {
    string[] public transactions;

    function TxRecord() {
    }

    event newRecord(string _data);

    function insertRecord(string _data) returns (bool success) {
        if (bytes(_data).length == 0) {
            throw;
        }
        transactions.push(_data);
        newRecord(_data);
        return true;
    }

    function() {
        throw;
    }
}



